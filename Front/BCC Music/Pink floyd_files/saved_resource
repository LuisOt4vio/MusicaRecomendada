  margin-top: -15px;
  width: 40px;
  height: 40px;
  border: 1px solid #eba54c;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  z-index: 10000000;
  opacity: .5;
  -webkit-transition: all .08s ease-out;
  -o-transition: all .08s ease-out;
